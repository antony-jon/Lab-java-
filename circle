public class circle {
	private double radius;
	private String color;
	circle()
	{
		radius=1.0;
		color="black";
	}
	circle(double radius){
		this.radius=radius;
	}
	circle(double radius,String color){
		this.radius=radius;
		this.color= color;
	}
	public double getRadius()
	{
		return radius;
	}
	public String getColor()
	{
		return color;
	}
	public void setRadius(double radius){
		this.radius=radius;
	}
	

}
